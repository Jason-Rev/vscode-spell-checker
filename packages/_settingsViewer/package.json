{
  "name": "code-spell-checker-settings-webview",
  "private": true,
  "version": "1.9.1",
  "description": "Webview for VS Code to edit cspell settings",
  "typings": "out",
  "main": "out",
  "scripts": {
    "build": "tsc -p . && npm run webpack",
    "clean": "rimraf ../client/settingsViewer ./out",
    "coverage": "jest --coverage src",
    "lint": "eslint 'src/**/*.ts'",
    "start:dev": "webpack-dev-server --config webpack.dev.js",
    "webpack": "webpack --env.production --config webpack.prod.js",
    "test": "jest src"
  },
  "keywords": [],
  "author": "Jason Dent",
  "license": "MIT",
  "devDependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@types/clone-deep": "^4.0.1",
    "@types/jest": "^26.0.14",
    "@types/node": "^12.12.68",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-test-renderer": "16.0.3",
    "awesome-typescript-loader": "^5.2.1",
    "browser-resolve": "^2.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "clone-deep": "^4.0.1",
    "eslint-plugin-react": "^7.21.4",
    "extract-loader": "^5.1.0",
    "file-loader": "^6.1.1",
    "handlebars": "^4.7.6",
    "handlebars-loader": "^1.7.1",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.5.3",
    "mobx": "^5.15.7",
    "mobx-react": "^6.3.1",
    "mobx-react-devtools": "^6.1.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-test-renderer": "^16.14.0",
    "resolve-url-loader": "^3.1.1",
    "terser": "^5.3.5",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3",
    "url-loader": "^4.1.1",
    "vscode-uri": "^2.1.2",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.2.0"
  },
  "files": [
    "out/api/**",
    "out/webapp/**",
    "!**/test*",
    "!**/*.map",
