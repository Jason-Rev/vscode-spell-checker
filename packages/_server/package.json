{
  "name": "vscode-spell-checker-server",
  "private": true,
  "description": "Spell Checker Server",
  "version": "1.7.21",
  "author": "Jason Dent",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/streetsidesoftware/vscode-spell-checker"
  },
  "engines": {
    "node": ">=8"
  },
  "devDependencies": {
    "@types/fs-extra": "^8.0.1",
    "@types/jest": "^24.9.1",
    "@types/jest-diff": "^20.0.1",
    "@types/lorem-ipsum": "^1.0.2",
    "@types/micromatch": "^3.1.1",
    "@types/node": "^11.15.4",
    "jest": "^24.9.0",
    "jest-diff": "^24.9.0",
    "lorem-ipsum": "^1.0.6",
    "ts-jest": "^24.3.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "comment-json": "^1.1.3",
    "cspell-glob": "^0.1.17",
    "cspell-lib": "^4.1.14",
    "fs-extra": "^8.1.0",
    "gensequence": "^2.3.0",
    "iconv-lite": "^0.4.24",
    "micromatch": "^4.0.2",
    "minimatch": "^3.0.4",
    "rxjs": "^6.5.4",
    "vscode-jsonrpc": "^4.0.0",
    "vscode-languageserver": "^5.2.1",
    "vscode-uri": "^2.1.1"
  },
  "scripts": {
    "clean": "rimraf ../client/server && rimraf ./tools/out",
    "lint": "tslint --force --format verbose \"src/**/*.ts\"",
    "build": "npm run compile && npm run installServerIntoExtension",
    "installServerIntoExtension": "installServerIntoExtension ../client ./package.json ./tsconfig.json",
    "clean-build": "npm run clean && npm run build",
    "clean-build-production": "npm run clean && npm run compile",
    "compile": "tsc -p .",
    "watch": "npm run installServerIntoExtension && tsc --watch -p .",
    "tsc": "tsc -p .",
    "test-watch": "jest --watch",
    "test": "jest"
  }
}
