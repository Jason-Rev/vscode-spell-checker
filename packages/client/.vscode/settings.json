{
    "cSpell.dictionaries": [
        "lorem-ipsum"
    ],
    "cSpell.enabledLanguageIds": [
        "c",
        "cpp",
        "csharp",
        "go",
        "html",
        "javascript",
        "javascriptreact",
        "json",
        "jsonc",
        "latex",
        "markdown",
        "php",
        "plaintext",
        "python",
        "text",
        "typescript",
        "typescriptreact",
        "yml"
    ],
    "cSpell.ignoreWords": [
        "arraylength",
        "languageclient",
        "restructuredtext"
    ],
    "cSpell.language": "en",
    "files.exclude": {
        "out": false
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        ".vscode-test": true,
        "server": true,
        "out": true
    },
    "typescript.tsdk": "./node_modules/typescript/lib"
}
